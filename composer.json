{
  "name": "gotit/gotit",
  "description": "Gene, Occurence and Taxa in Integrative Taxonomy",
  "type": "project",
  "license": "proprietary",
  "require": {
    "php": "^8.0",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "api-platform/core": "^2.6",
    "babdev/pagerfanta-bundle": "2.*",
    "composer/package-versions-deprecated": "1.*",
    "doctrine/annotations": "1.*",
    "doctrine/doctrine-bundle": "2.*",
    "doctrine/doctrine-migrations-bundle": "^3.1",
    "doctrine/orm": "2.*",
    "friendsofsymfony/jsrouting-bundle": "2.*",
    "gedmo/doctrine-extensions": "^3.1",
    "league/csv": "<9.7",
    "myclabs/php-enum": "^1.7.7",
    "nelmio/cors-bundle": "^2.1",
    "phpdocumentor/reflection-docblock": "^5.2",
    "sensio/framework-extra-bundle": "6.*",
    "stof/doctrine-extensions-bundle": "^1.6",
    "symfony/apache-pack": "1.*",
    "symfony/asset": "5.*",
    "symfony/console": "5.*",
    "symfony/dotenv": "5.*",
    "symfony/expression-language": "5.*",
    "symfony/flex": "1.*",
    "symfony/form": "5.*",
    "symfony/framework-bundle": "5.*",
    "symfony/intl": "5.*",
    "symfony/property-access": "5.*",
    "symfony/property-info": "5.*",
    "symfony/proxy-manager-bridge": "5.*",
    "symfony/runtime": "5.*",
    "symfony/security-bundle": "5.*",
    "symfony/serializer": "5.*",
    "symfony/twig-bundle": "5.*",
    "symfony/validator": "5.*",
    "symfony/webpack-encore-bundle": "1.*",
    "symfony/yaml": "5.*",
    "willdurand/js-translation-bundle": "4.*"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/GOTIT-DEV/symplify-cs-fixer"
    }
  ],
  "require-dev": {
    "codeception/codeception": "^4.1",
    "codeception/module-asserts": "^1.3",
    "codeception/module-datafactory": "^1.1",
    "codeception/module-db": "^1.1",
    "codeception/module-doctrine2": "^1.1",
    "codeception/module-phpbrowser": "^1.0",
    "codeception/module-rest": "^1.3",
    "codeception/module-symfony": "^2.0",
    "friendsofphp/php-cs-fixer": "^3.2",
    "rector/rector": "^0.11.53",
    "suin/symplify-cs-fixer": "^3",
    "symfony/maker-bundle": "1.*",
    "symfony/stopwatch": "5.*",
    "symfony/web-profiler-bundle": "5.*",
    "symplify/coding-standard": "^9.4"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "paragonie/random_compat": "2.*",
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "5.*"
    }
  }
}
