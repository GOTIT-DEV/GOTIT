{
  "name": "gotit/gotit",
  "description": "Gene, Occurence and Taxa in Integrative Taxonomy",
  "type": "project",
  "license": "proprietary",
  "require": {
    "php": ">=7.1.0",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "composer/package-versions-deprecated": "1.*",
    "doctrine/annotations": "1.*",
    "doctrine/doctrine-bundle": "2.*",
    "doctrine/orm": "2.*",
    "friendsofsymfony/jsrouting-bundle": "2.*",
    "myclabs/php-enum": "1.*",
    "sensio/framework-extra-bundle": "6.*",
    "symfony/apache-pack": "1.*",
    "symfony/asset": "5.*",
    "symfony/console": "5.*",
    "symfony/dotenv": "5.*",
    "symfony/expression-language": "5.*",
    "symfony/flex": "1.*",
    "symfony/form": "5.*",
    "symfony/framework-bundle": "5.*",
    "symfony/intl": "5.*",
    "symfony/runtime": "5.*",
    "symfony/security-bundle": "5.*",
    "symfony/serializer-pack": "1.*",
    "symfony/twig-bundle": "5.*",
    "symfony/validator": "5.*",
    "symfony/webpack-encore-bundle": "1.*",
    "symfony/yaml": "5.*",
    "willdurand/js-translation-bundle": "4.*"
  },
  "require-dev": {
    "symfony/maker-bundle": "1.*",
    "symfony/profiler-pack": "1.*"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "paragonie/random_compat": "2.*",
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "5.*"
    }
  }
}
