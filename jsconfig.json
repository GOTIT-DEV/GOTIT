{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "~Components/*": [
        "assets/components/*"
      ],
      "~Core/*": [
        "assets/Core/*"
      ],
      "~QueryBuilder/*": [
        "assets/QueryBuilder/*"
      ],
      "~SpeciesSearch/*": [
        "assets/SpeciesSearch/*"
      ],
      "~Public/*": [
        "public/*"
      ],
    }
  },
  "exclude": [
    "node_modules",
    "vendor",
    "var",
    "dist"
  ]
}